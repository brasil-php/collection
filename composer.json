{
  "name": "php-brasil/collection",
  "description": "",
  "minimum-stability": "stable",
  "type": "library",
  "version": "0.2.6",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "PhpBrasil\\Collection\\": "src/"
    },
    "files": [
      "bootstrap/helper.php",
      "bootstrap/alias.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PhpBrasil\\Collection\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "major": "./composer.release.sh major",
    "minor": "./composer.release.sh minor",
    "patch": "./composer.release.sh patch"
  }
}
